{
  "name": "Zenterprise",
  "version": "0.0.1",
  "description": "Zenerprise restaurant enterprise resource planning app",
  "main": "index.js",
  "private": false,
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "gulp lint",
    "test": "gulp mocha &&  gulp lint",
    "commit": "git-cz",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postinstall": "gulp",
    "heroku-postbuild": "gulp",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{html,js,jsx,less,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongodb-session": "^2.4.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "debug": "^4.1.1",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "express-session": "^1.17.1",
    "express-validation": "1.0.2",
    "express-winston": "^3.2.0",
    "helmet": "^3.23.3",
    "http-status": "^1.3.2",
    "joi": "14.3.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.6.1",
    "morgan": "1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "path": "^0.12.7",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.23.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "^4.2.0",
    "commitizen": "^4.2.4",
    "coveralls": "^3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "del": "^4.1.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-promise": "^4.2.1",
    "ghooks": "^2.0.4",
    "gulp": "^4.0.2",
    "gulp-babel": "7.0.1",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^5.0.0",
    "gulp-istanbul": "1.1.3",
    "gulp-load-plugins": "^1.6.0",
    "gulp-mocha": "^8.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "husky": "^3.1.0",
    "isparta": "^4.1.1",
    "lint-staged": "^9.5.0",
    "mocha": "^8.0.0",
    "prettier": "1.19.1",
    "run-sequence": "^2.2.1",
    "supertest": "^4.0.2",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
